#include <iostream>
using namespace std;
int area(int length, int width);
struct Reactangle {
  int length;
  int width;
};
int main() {

  Reactangle yard, house;

  cout << "Enter length of Yard:";
  cin >> yard.length;
  cout << "Enter width of Yard:";
  cin >> yard.width;

  cout << "Enter length of House:";
  cin >> house.length;
  cout << "Enter width of House:";
  cin >> house.width;
  cout << "Area is :"
       << area(yard.length, yard.width) - area(house.length, house.width);
  ;

  return 0;
}
int area(int length, int width) {
  int Area;
  return Area = length * width;
}
